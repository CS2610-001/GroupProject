<div class="wrap">
  <header class = "gp-header">
    <div class="gp-squeeze">
      <h1 class= "pull--left">#{{search_tag}}</h1>
      <h4 class ="pull--right"> welcome {{userName}}! </h4>
    </div>
  </header>
  <div class="nav-wrap">
    <div class="gp-squeeze">
      {{>menu}}
    </div>
  </div>
  <div class="content">
    <div class="gp-squeeze">
      <form id="saveSearch" action="/tags/save" method="post">
        <input type="hidden" class = "search-text" value={{search_tag}}>
        <input type="submit" class="save-button" value="save">
      </form>
      {{>feed}}
    </div>
  </div>
  <div class="push"></div>
</div>
<footer class ="gp-footer">
  <div class="gp-squeeze">
    <h4>Footer</h4>
  </div>
</footer>
