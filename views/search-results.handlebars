<div class="wrap">
  <header class = "gp-header">
    <div class="gp-squeeze">
      <h1 class= "pull--left">#{{search_tag}}</h1>
      <form class="tag-form" id="saveSearch" action="/tags/save" method="post">
          <p><input type="hidden" class = "search-text" name="tag" value={{search_tag}}></p>
          <p><input type="submit" class="save-button" value="save tag"></p>
        </form>
      <h4 class ="pull--right"> welcome {{userName}}!
        <a href="/logout" class="logout">Logout </a>
      </h4>
    </div>
  </header>
  <div class="nav-wrap">
    <div class="gp-squeeze">
      {{>menu}}
    </div>
  </div>
  <div class="content">
    <div class="gp-squeeze">
      {{>feed}}
    </div>
  </div>
  <div class="push"></div>
</div>
<footer class ="gp-footer">
  <div class="gp-squeeze">
    <h4>Footer</h4>
  </div>
</footer>
